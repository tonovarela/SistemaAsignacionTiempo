<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import de.jensd.fx.glyphs.fontawesome.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" stylesheets="@../resources/principal.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.SAT.Controllers.FXMLCancelarTiempoController">
   <children>
      <Pane fx:id="panelModal" layoutX="-1.0" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Button fx:id="ButtonAceptar" layoutX="93.0" layoutY="273.0" mnemonicParsing="false" onAction="#AceptarBtnAction" prefHeight="57.0" prefWidth="156.0" text="Aceptar">  
                <graphic>
                    <FontAwesomeIcon fill="#2c8339" glyphName="CHECK" size="1.5em" /> 
                </graphic>
                </Button>
            <Label fx:id="labelCancelarTiempo" layoutX="59.0" layoutY="120.0" text="Confirma cancelar su tiempo?" />
            <Button fx:id="ButtonCancelar" layoutX="381.0" layoutY="273.0" mnemonicParsing="false" onAction="#CancelarBtnAction" prefHeight="57.0" prefWidth="156.0" text="Cancelar">
                <graphic>
                    <FontAwesomeIcon fill="#f51717" glyphName="REMOVE" size="1.5em" /> 
                </graphic>
                </Button>
         </children>
      </Pane>
   </children>
</AnchorPane>
